package 'wireless'

<% access_point.radios.each do |radio| -%>

<% if radio.driver == 'madwifi-ng' -%>
<%= render :partial => 'access_points/uci_wireless_madwifi',  :locals => { :radio => radio } %>
<% elsif radio.driver == 'mac80211' -%>
<%= render :partial => 'access_points/uci_wireless_mac80211', :locals => { :radio => radio } %>
<% end -%>

<% end -%>
