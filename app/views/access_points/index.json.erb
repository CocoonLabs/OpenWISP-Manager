[<%=
    json_access_points = []
    @access_points.each do |ap|
        if params[:except].blank? or params[:except].to_i != ap.id
            json_access_points << { :access_point => {
                :id => ap.id,
                :name => ap.name,
                :lat => ap.lat,
                :lon => ap.lon,
                :address => ap.address,
                :city => ap.city,
                :url => wisp_access_point_path(@wisp, ap)
            }}.to_json
        end
    end
    json_access_points.join(',')
%>]

