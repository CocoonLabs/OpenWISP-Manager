<% access_point_template = AccessPointTemplate.find(access_point_template_id)%> 
<% if ( (access_point_template.access_points.length != 0 ) and !access_point_template.is_committed? ) %>
<div id="list_access_points_div"> 
  <a><%= t(:Listing_Access_Points_Modified) %></a>
	</br>
 
  <table id="access-points-table">
  <thead>
    <tr>
      <th scope="col" class="head-left"><%= t(:Name) %></th>
      <th scope="col" class="head"><%= t(:Address) %></th>
      <th scope="col" class="head"><%= t(:Zip) %></th>
      <th scope="col" class="head-right"><%= t(:City) %></th>
    </tr>
  </thead>
  <tbody>
	
	<% access_point_template.access_points.each do |ap|%>
      <tr>
	    <td><%= ap.name %></td>
        <td><%= ap.address %></td>
        <td><%= ap.zip %></td>
        <td><%= ap.city %></td>
	  </tr>
	<% end %>
  </tbody>
 </table>

</div>
<% end %>
